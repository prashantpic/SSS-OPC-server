{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information"
      }
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5201" // Example different port for local dev
      },
      "Grpc": {
        "Url": "http://localhost:5202", // Example different gRPC port for local dev
        "Protocols": "Http2"
      }
    }
  },
  "FeatureManagement": {
    "EnableMlflowIntegration": true // Example: Enable MLflow for development
  },
  "ServiceEndpoints": {
    "DataServiceGrpcUrl": "http://localhost:9091" // Dev DataService gRPC URL
  },
  "ModelOptions": {
    "ModelStorageBasePath": "ModelsData" // Relative path for local model storage during development
  },
  "NlpProviderOptions": {
    "ActiveProvider": "SpaCy", // Can be overridden for specific dev testing
    "AzureCognitiveServices": {
      "ApiKey": "YOUR_DEV_AZURE_COGNITIVE_SERVICES_API_KEY", // Use User Secrets for actual dev keys
      "Endpoint": "YOUR_DEV_AZURE_COGNITIVE_SERVICES_ENDPOINT",
      "LanguageResourceName": "dev-language-resource",
      "DeploymentName": "dev-clu-deployment"
    }
  },
  "MLOpsOptions": {
    "Mlflow": {
      "TrackingUri": "http://localhost:5000" // Local MLflow instance
    },
    "AzureML": {
      "WorkspaceName": "dev-ml-workspace",
      "SubscriptionId": "dev-subscription-id",
      "ResourceGroup": "dev-resource-group"
    },
    "ApiKey": "DEV_MLOPS_API_KEY" // Use User Secrets for actual dev keys
  }
}