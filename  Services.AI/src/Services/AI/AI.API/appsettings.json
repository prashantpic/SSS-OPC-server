{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DataServiceGrpc": "http://dataservice:80"
  },
  "Azure": {
    "CognitiveServices": {
      "Endpoint": "YOUR_AZURE_COGNITIVE_SERVICES_ENDPOINT",
      "ApiKey": "YOUR_AZURE_COGNITIVE_SERVICES_API_KEY",
      "ProjectName": "YOUR_CLU_PROJECT_NAME",
      "DeploymentName": "YOUR_CLU_DEPLOYMENT_NAME"
    },
    "Storage": {
      "ConnectionString": "DefaultEndpointsProtocol=https;AccountName=youraccount;AccountKey=yourkey;EndpointSuffix=core.windows.net",
      "ModelContainerName": "ai-models"
    }
  },
  "MlOps": {
    "Mlflow": {
      "TrackingUri": "http://mlflow:5000"
    }
  },
  "FeatureToggles": {
    "UseGoogleNlp": false,
    "EnableModelExplainability": true,
    "EnableCustomModelFineTuning": false
  }
}